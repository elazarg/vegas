(declare-const Even_c Bool)
(declare-const Even_c_done Bool)

(declare-const Odd_c Bool)
(declare-const Odd_c_done Bool)

(declare-const action_Odd_1_done Bool)
(declare-const action_Even_3_done Bool)
(declare-const action_Odd_2_done Bool)
(declare-const action_Even_4_done Bool)
(declare-const Even_outcome Int)
(assert (= Even_outcome (ite (and Even_c_done Odd_c_done) (ite (= Even_c Odd_c) 126 74) (ite (and (not Even_c_done) Odd_c_done) 20 (ite (and Even_c_done (not Odd_c_done)) 180 100)))))
(declare-const Odd_outcome Int)
(assert (= Odd_outcome (ite (and Even_c_done Odd_c_done) (ite (= Even_c Odd_c) 74 126) (ite (and (not Even_c_done) Odd_c_done) 180 (ite (and Even_c_done (not Odd_c_done)) 20 100)))))
(assert (=> action_Odd_1_done (and true true)))
(assert (=> action_Odd_1_done Odd_c_done))
(assert (=> action_Even_3_done (and true true)))
(assert (=> action_Even_3_done Even_c_done))
(assert (=> action_Odd_2_done (and (and (and true action_Odd_1_done) action_Even_3_done) true)))
(assert (=> action_Odd_2_done Odd_c_done))
(assert (=> action_Even_4_done (and (and (and true action_Even_3_done) action_Odd_1_done) true)))
(assert (=> action_Even_4_done Even_c_done))
(check-sat)
(get-model)
