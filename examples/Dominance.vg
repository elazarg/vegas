// Dominance: A always gets more regardless of choices (test case for strategy dominance)
// Distribution semantics: pot (20 wei) favors A in all outcomes

game main() {
  join A() $ 10;
  join B() $ 10;
  yield A(c: bool) B(c: bool);
  withdraw (A.c != B.c)
      ? { A -> 15; B -> 5 }
      : { A -> 15; B -> 5 }
}
