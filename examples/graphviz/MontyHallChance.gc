  splines=true;
  subgraph "cluster_0" {
    label="Guest";
    style=dashed;
    color="#777777";
    fontcolor="#333333";
    "n_Guest_1" [label=<<table border="0" cellborder="0" cellspacing="0"><tr><td><font point-size="8" color="#555555">#1</font></td></tr><tr><td><font color="#AAAAAA">&#8212;</font></td></tr></table>>, shape=box, fillcolor="#F0F0F0", penwidth=0.5];
