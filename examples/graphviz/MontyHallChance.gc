  "n_Guest_1" -> "n_Host_2";
  "n_Host_2" -> "n_Guest_3";
  "n_Guest_3" -> "n_Host_4";
  "n_Host_2" -> "n_Host_4";
  "n_Host_4" -> "n_Guest_5";
  "n_Guest_5" -> "n_Host_6";
  "n_Host_2" -> "n_Host_6";
