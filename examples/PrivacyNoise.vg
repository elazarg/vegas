// Privacyâ€“noise game: querier asks; owner chooses noisy or accurate response at different costs/benefits.
join Owner() $ 100 Querier() $ 100;

yield Querier(query: bool);
yield Owner(noise: bool) where Querier.query;

withdraw {
    Querier -> (!Querier.query ? 0 : (Owner.noise ? 1 : 5));
    Owner   -> (!Querier.query ? 0 : (Owner.noise ? -1 : -3));
}
